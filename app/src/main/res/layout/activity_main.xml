<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnListaBanco"
        android:layout_width="102dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="183dp"
        android:layout_marginTop="295dp"
        android:onClick="getListaBancos"
        android:text="Lista Bancos" />

    <TextView
        android:id="@+id/txtNombre"
        android:layout_width="46dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="41dp"
        android:layout_marginTop="145dp"
        android:text="Clave"
        android:textAlignment="center"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/lblTITULO"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:autoSizeTextType="none"
        android:autoText="false"
        android:background="#FAF6F6"
        android:backgroundTint="#E3E5F1"
        android:text="PRACTICA 1: Inicio Sesion"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="95dp"
        android:text="NOMBRE: "
        android:textAlignment="center"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnListaBanco"
        android:layout_width="102dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="94dp"
        android:layout_marginTop="218dp"
        android:onClick="btnEnviarWs"
        android:text="Ingresar WS" />

    <Button
        android:id="@+id/btnListaBanco"
        android:layout_width="102dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="273dp"
        android:layout_marginTop="214dp"
        android:onClick="btnIngreVolly"
        android:text="Ingresar Volly" />

    <EditText
        android:id="@+id/txtUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="54dp"
        android:ems="10"
        android:inputType="text" />

    <TextView
        android:id="@+id/txtVis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="427dp"
        android:text="TextView" />

    <EditText
        android:id="@+id/txtClave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="58dp"
        android:ems="10"
        android:inputType="textPassword" />
</RelativeLayout>